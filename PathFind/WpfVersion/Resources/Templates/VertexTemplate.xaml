<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:WpfVersion.Resources.Templates"
                    x:Name="TemplateDictionary">
    <ControlTemplate x:Key="vertexTemplate" TargetType="Label">
        <Grid>
            <Rectangle Fill="{TemplateBinding Background}" Stroke="{TemplateBinding Foreground}" />
            <Border BorderBrush="White" BorderThickness="1"></Border>
            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
        </Grid>
    </ControlTemplate>
</ResourceDictionary>